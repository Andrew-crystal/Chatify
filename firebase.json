{
	"hosting": {
		"headers": [
			{
				"source": "**/*.@(jpg|jpeg|gif|png|svg|css|js)",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "max-age=7200"
					},
					{
						"key": "Access-Control-Allow-Origin",
						"value": "*"
					}
				]
			}
		],
		"public": "build",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
	}
}
